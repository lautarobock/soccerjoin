{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html"
        ],
        "versionedFiles": [
          "/*.bundle.css",
          "/*.bundle.js",
          "/*.chunk.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    },
    {
      "name": "profile",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "https://graph.facebook.com/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "from-api",
      "urls": [
        "https://soccerjoin.herokuapp.com/api/matches",
        "https://soccerjoin.herokuapp.com/api/users"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 15,
        "maxAge": "1h",
        "timeout": "5s"
      }
    }
  ]
}